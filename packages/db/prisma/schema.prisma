

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id  String @id @default(uuid())
  email String
  password String
  name String
  photo String
}

model Room{
  id Int @id @default(autoincrement())
  slug String @unique
  createAt DateTime @default(now())
}

model chat{
  id Int @id @default(autoincrement()())
}

db.room.create()